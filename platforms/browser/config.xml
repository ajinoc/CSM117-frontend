<?xml version='1.0' encoding='utf-8'?>
<widget id="com.telestrations" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Telestrations</name>
    <description>
        CSM117 Group Project
    </description>
    <author email="adam.jones@ucla.edu" href="https://github.com/ajinoc/CSM117">
        Telestrations Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
