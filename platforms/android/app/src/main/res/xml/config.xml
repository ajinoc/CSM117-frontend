<?xml version='1.0' encoding='utf-8'?>
<widget id="com.telestrations" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Telestrations</name>
    <description>
        CSM117 Group Project
    </description>
    <author email="adam.jones@ucla.edu" href="https://github.com/ajinoc/CSM117-frontend">
        Telestrations Team
    </author>
    <content src="index.html" />
    <access origin="https://telestrations-csm117.herokuapp.com/*" />
    <access origin="wss://telestrations-csm117.herokuapp.com/*" />
    <allow-navigation href="https://telestrations-csm117.herokuapp.com/*" />
    <allow-navigation href="wss://telestrations-csm117.herokuapp.com/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="res/icon.png" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
